{
  "name": "@quarkly/elements",
  "description": "atomic react UI components",
  "author": "Eddort",
  "user": "Eddort",
  "version": "0.5.5",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf build && mkdir build",
    "build:cjs": "NODE_ENV=cjs babel src --out-dir build/cjs --ignore src/**/*.stories.js,src/**/*.test.js,src/storyModules ",
    "build:esm": "NODE_ENV=esm babel src --out-dir build/esm --ignore src/**/*.stories.js,src/**/*.test.js,src/storyModules ",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ignore-path .gitignore"
  },
  "main": "build/cjs",
  "module": "build/esm",
  "peerDependencies": {
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "latest",
    "@storybook/addon-storyshots": "latest",
    "@storybook/react": "latest",
    "@storybook/theming": "^5.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.1.2",
    "core-js": "^3.1.3",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "prettier": "1.16.4",
    "prop-types": "^15.7.2",
    "react-icons": "^3.5.0",
    "react-popper": "^1.3.3",
    "styled-components": "4.2.0"
  },
  "keywords": [
    "react",
    "quarkly",
    "styled-components",
    "atomic-components",
    "quarkly/elementary"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quarkly/elements"
  },
  "bugs": {
    "url": "https://github.com/quarkly/elements/issues"
  },
  "homepage": "https://github.com/quarkly/elements#readme",
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@quarkly/elementary": "^1.1.4",
    "classnames": "^2.2.6",
    "lodash": "latest",
    "react": "^16.8.1",
    "react-dom": "^16.8.2",
    "react-slick": "^0.23.2",
    "slick-carousel": "^1.8.1"
  }
}
