{
  "name": "@quarkly/elements",
  "description": "quarkly elements",
  "author": "Eddort",
  "user": "Eddort",
  "version": "0.0.5",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build:elements": "webpack --mode production --config ./config/webpack.config.js",
    "build:hydrate": "webpack --mode production --config ./config/hydrate.config.js",
    "build": "npm run build:elements && npm run build:hydrate",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:lint": "eslint . --ignore-path .gitignore"
  },
  "main": "build",
  "peerDependencies": {
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.1",
    "jest": "^22.4.2",
    "prettier": "1.16.4",
    "prop-types": "^15.7.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "styled-components": "4.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "quarkly"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "latest",
    "qslot": "0.0.5",
    "react": "^16.8.1",
    "react-dom": "^16.8.2",
    "react-slick": "^0.23.2",
    "slick-carousel": "^1.8.1",
    "styled-system": "latest"
  }
}
